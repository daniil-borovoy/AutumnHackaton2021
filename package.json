{
  "name": "autumnhackaton2021",
  "version": "1.0.0",
  "description": "Fantastic hackaton",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix frontend",
    "client:install": "npm install --prefix frontend",
    "client:build": "npm run build --prefix frontend",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Daniel-Borovoy/AutumnHackaton2021.git"
  },
  "keywords": [
    "React",
    "Express",
    "js"
  ],
  "author": "Reactor",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "mongoose": "^6.0.12"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14"
  }
}
